# Ignore files generated by the IDE
.idea/

# Ignore compiled binaries
*.class
*.jar

# Ignore build output directories
/build/
/dist/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore logs and temporary files
*.log
*.tmp

# Ignore dependencies
/node_modules/
/.venv/

# Ignore user-specific files
/.env
/.env.local
/.env.*.local

# Ignore package lock files
package-lock.json
yarn.lock
